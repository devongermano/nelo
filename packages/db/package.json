{
  "name": "@nelo/db",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "postinstall": "prisma generate",
    "generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev --skip-seed",
    "db:migrate:create": "prisma migrate dev --create-only --skip-seed",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:reset": "prisma migrate reset --force --skip-seed",
    "db:push": "echo 'WARNING: db push is for prototyping only!' && prisma db push",
    "db:seed": "prisma db seed",
    "db:test:setup": "DATABASE_URL=\"postgresql://nelo:nelo@localhost:5432/nelo_test\" prisma migrate deploy",
    "db:test:reset": "DATABASE_URL=\"postgresql://nelo:nelo@localhost:5432/nelo_test\" prisma migrate reset --force --skip-seed",
    "seed": "node --loader ts-node/esm seed.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2"
  },
  "devDependencies": {
    "prisma": "^5.10.2",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm src/seed.ts"
  }
}
